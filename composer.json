{
  "name": "paulorlima9/laravel-brazilian-validations",
  "version": "1.0.0",
  "description": "A library containing validations for Brazilian formats, for Laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Paulo R. Lima",
      "email": "paulorlima333@gmail.com"
    }
  ],
  "require": {
    "illuminate/support": "^11.0 || ^12.0"
  },
  "autoload": {
    "psr-4": {
      "PauloRLima\\brazilian-validations\\": "src/validators"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "PauloRLima\\BrazilianValidations\\ValidatorProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "require-dev": {
    "pestphp/pest": "^1.21"
  },
  "scripts": {
    "test": "./vendor/bin/pest"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
